set.seed(123)
differences <- replicate(1000, # do this 1000 times
{
groups <-rbinom(40, 1, 0.5) # randomize patients
nGroupA <- sum(groups) # how many are in group A
nGroupB <- 40 - nGroupA # how many are in group B
return(nGroupA - nGroupB) # return the difference
})
par(las=1) hist(differences+0.5, breaks=-20.5:20.5, col = "grey",
xlab = "difference in group size")
par(las=1)
hist(differences+0.5, breaks=-20.5:20.5, col = "grey",
xlab = "difference in group size")
